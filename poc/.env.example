# E2B Configuration
# Get your API key from https://e2b.dev
E2B_API_KEY=your-e2b-api-key-here

# Optional: ScaleBox Configuration (alternative to E2B)
# E2B_API_KEY=your-scalebox-api-key
# E2B_API_URL=https://api.scalebox.dev/v1

# Strands Agent Configuration
# Required: OpenAI-compatible API Key (e.g., Together AI)
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Base URL for OpenAI-compatible API
OPENAI_BASE_URL=https://api.together.xyz/v1

# Optional: Model name (use OpenAI-compatible model format)
MODEL_NAME=openai/gpt-oss-120b

# Optional: Temperature for LLM responses (default: 0.3)
TEMPERATURE=0.3

# Optional: Maximum tokens for LLM responses (default: 4000)
MAX_TOKENS=4000
